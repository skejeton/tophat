/**
 * Toml tests (umka/toml.um)
 */

import ("toml.um")

fn expectToEqual(a, b: str) {
  if a != b {
    error(sprintf("'%s' != '%s'", a, b))
  }
  printf("Ok\n")
}

const input = "\n"+
"a-_ = \"Dam\"\n"+
"1 = \"A\\\"hh\""+
" = \"Bruh\"";

fn main() {
  result := toml.parse(input)

  expectToEqual(result.data["a-_"], "Dam")
  expectToEqual(result.data["1"], "A\"hh")
}