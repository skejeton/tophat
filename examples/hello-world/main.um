
import (
	"th.um"
	"rect.um"
	"canvas.um"
	"window.um"

	"global.um"
)

fn main() {
	window.setup("title", 1280, 720)

	global.cam = rect.mk(0, 0, 192, 108)

	for window.cycle(global.cam) {
		canvas.drawRect(th.black, rect.mk(0, 0, cam.w, cam.h))

		canvas.drawText("hello world", th.Vf2{2, 2}, th.white, 2)
	}
}
